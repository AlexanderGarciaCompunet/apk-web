{
  "name": "arknova/cititor-server",
  "description": "Yii 2 Advanced Project Template + AdminLTE + RBAC",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "project template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1.0",
    "yiisoft/yii2": "~2.0.41",
    "yiisoft/yii2-bootstrap4": "@dev",
    "yiisoft/yii2-swiftmailer": "~2.1.2",
    "kartik-v/yii2-widget-select2": "dev-master",
    "kartik-v/yii2-grid": "dev-master",
    "kartik-v/yii2-mpdf": "dev-master",
    "kartik-v/yii2-export": "dev-master",
    "mdmsoft/yii2-admin": "dev-master",
    "kartik-v/yii2-widget-depdrop": "dev-master",
    "kartik-v/yii2-date-range": "*",
    "kartik-v/yii2-editable": "*",
    "kartik-v/yii2-widget-colorinput": "*",
    "kartik-v/yii2-widget-datepicker": "dev-master",
    "kartik-v/yii2-field-range": "*",
    "kartik-v/yii2-widget-fileinput": "dev-master",
    "kartik-v/yii2-number": "dev-master",
    "seisvalt/yii2-adminlte-asset": "dev-master",
    "seisvalt/change-log-behavior": "*",
    "wbraganca/yii2-dynamicform": "*",
    "bower-asset/fontawesome": "~5",
    "ogheo/yii2-comments": "*",
    "yii2tech/authlog": "*",
    "yii2mod/yii2-rbac": "*",
    "bizley/migration": "^4.0",
    "sam-it/yii2-mariadb": "^2.0",
    "mootensai/yii2-enhanced-gii": "dev-master",
    "kartik-v/yii2-tree-manager": "dev-master",
    "kartik-v/yii2-datecontrol": "@dev",
    "2amigos/yii2-chartjs-widget": "~2.0",
    "kartik-v/yii2-widget-spinner": "dev-master",
    "bower-asset/viewerjs": "*",
    "kartik-v/yii2-checkbox-x": "dev-master",
    "ext-json": "*",
    "wisembly/elephant.io": "^3.3",
    "yiisoft/yii2-redis": "^2.0",
    "firebase/php-jwt": "^5.2",
    "dominus77/yii2-sweetalert2-widget": "~1.3",
    "kartik-v/yii2-bootstrap4-dropdown": "@dev",
    "arknova/encrypter": "*",
    "edvlerblog/yii2-adldap-module": "@dev",
    "kartik-v/yii2-bootstrap5-dropdown": "dev-master",
    "kartik-v/yii2-dropdown-x": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "~2.1.16",
    "yiisoft/yii2-gii": "~2.2.0",
    "yiisoft/yii2-faker": "~2.0.5",
    "codeception/verify": "^2.0"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "enabled": false
    },
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/seisvalt/change-log-behavior"
    },
    {
      "type": "vcs",
      "url": "https://github.com/seisvalt/yii2-adminlte-asset"
    },
    {
      "type": "vcs",
      "url": "https://bitbucket.org/arknova-qa/yii2-adldap-module.git"
    },
    {
      "type": "vcs",
      "url": "https://bitbucket.org/arknova-col/encrypter.git"
    }
  ]
}
